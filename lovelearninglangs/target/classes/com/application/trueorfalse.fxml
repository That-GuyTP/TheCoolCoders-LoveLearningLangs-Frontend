<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<!--This is our Basic question implemenation for true or false-->

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.TrueOrFalse">
    <children>
        <Label text="True or False" />
        <Label text="Example Question" />
        <Button fx:id="trueButton" mnemonicParsing="false" text="True" />
        <Button fx:id="falseButton" mnemonicParsing="false" text="False" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>